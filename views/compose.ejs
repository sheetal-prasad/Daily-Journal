<%- include("partials/header") -%>
<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label for="title">Title</label>
    <input class="form-control" id="title" type="text" name="postTitle">
    </div>
    <div class="form-group">
    <label for="body">Post</label>
    <textarea class="form-control input-lg" id="body" rows="5" type="textbox" name="postBody" ></textarea>
  </div>
  <button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>
<%- include("partials/footer") -%>
